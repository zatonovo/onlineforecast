@Article{Cao2008,
  author    = {Cao, Jiacong and Lin, Xingchun},
  title     = {Study of hourly and daily solar irradiation forecast using diagonal recurrent wavelet neural networks},
  journal   = {Energy Conversion and Management},
  year      = {2008},
