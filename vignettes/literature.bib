@Article{Charytoniuk1998,
  Title                    = {Nonparametric regression based short-term load forecasting},
  Author                   = {Charytoniuk, W. and Chen, M.-S. and Van Olinda, P.},
  Journal                  = {Power Systems, IEEE Transactions on},
