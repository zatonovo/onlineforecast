@Article{Mueller2004,
  author    = {R.W. Mueller and K.F. Dagestad and P. Ineichen and M. Schroedter-Homscheidt and S. Cros and D. Dumortier and R. Kuhlemann and J.A. Olseth and G. Piernavieja and C. Reise and L. Wald and D. Heinemann},
